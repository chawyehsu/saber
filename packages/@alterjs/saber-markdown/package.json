    "lint": "eslint src",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@babel/core": "^7.26.0",
    "@babel/helper-plugin-utils": "^7.26.5",
    "@types/babel__core": "^7.20.5",
    "@types/babel__helper-plugin-utils": "^7.10.3",
    "@types/markdown-it": "^14.1.1",
    "builtin-modules": "^3.3.0",
    "markdown-it": "^14.1.0"
